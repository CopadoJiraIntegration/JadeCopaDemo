    </fields>
    <fields>
        <fullName>amazonconnect__disableCCAContact__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Choose to enable or disable the AC_AssociateCCAContact trigger, which associates a Contact with a Contact Channel Analytics record.</description>
        <externalId>false</externalId>
        <inlineHelpText>Choose to enable or disable the AC_AssociateCCAContact trigger, which associates a Contact with a Contact Channel Analytics record.</inlineHelpText>
        <label>Disable the CCA Contact Trigger</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>

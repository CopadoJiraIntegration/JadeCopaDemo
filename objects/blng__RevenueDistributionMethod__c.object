    <validationRules>
        <fullName>blng__CannotSpecifyRevDatesifTransTypeAutomati</fullName>
        <active>true</active>
        <errorConditionFormula>((ISPICKVAL(blng__RevenueTransactionCreationProcess__c  , &apos;Automatic&apos;))  &amp;&amp;  
(ISPICKVAL(blng__RevenueScheduleTermStartDate__c, &apos;Manual&apos;) ))
 || 
(ISPICKVAL(blng__RevenueTransactionCreationProcess__c, &apos;Automatic&apos;)
&amp;&amp;
(ISPICKVAL(blng__RevenueScheduleTermEndDate__c, &apos;Manual&apos;)))</errorConditionFormula>
        <errorMessage>Cannot select Manual on Revenue Schedule Term Start Date or Revenue Schedule End Date when Revenue Transaction Creation Process is Automatic</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>blng__DontSpecifyDatesIfCreationProcessManual</fullName>
        <active>true</active>
        <errorConditionFormula>((!ISPICKVAL(blng__DistributionMethod__c, &apos;Full Recognition&apos;) )
|| 
(!ISPICKVAL(blng__DistributionMethod__c, &apos;Manual&apos;)))
 &amp;&amp; 
(ISPICKVAL( blng__RevenueTransactionCreationProcess__c, &apos;Manual&apos;) )
 &amp;&amp; 
((ISPICKVAL(blng__RevenueScheduleTermStartDate__c, &apos;Invoice Line Start Date&apos;))
 || 
(ISPICKVAL( blng__RevenueScheduleTermStartDate__c, &apos;Order Product Start Date&apos;))) 

 || 

(ISPICKVAL( blng__RevenueTransactionCreationProcess__c, &apos;Manual&apos;) )
 &amp;&amp; 
((ISPICKVAL(blng__RevenueScheduleTermEndDate__c, &apos;Invoice Line End Date&apos;))
 || 
(ISPICKVAL( blng__RevenueScheduleTermEndDate__c, &apos;Order Product End Date&apos;)))</errorConditionFormula>
        <errorMessage>Revenue Schedule Start Date and End Date should only be manual when Revenue Transaction Creation Process is Manual</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>blng__DontSpecifyFRDateIfCreationProcessManual</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(blng__DistributionMethod__c, &apos;Full Recognition&apos;)
&amp;&amp;
ISPICKVAL( blng__RevenueTransactionCreationProcess__c, &apos;Automatic&apos;)
&amp;&amp;
ISPICKVAL(  blng__FullRecognitionDate__c , &apos;Manual&apos;)</errorConditionFormula>
        <errorMessage>Full Recognition date cannot be Manual when Revenue Transaction Creation Process is Automatic</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>blng__DontSpecifyStartDateEndDateForFullRecogn</fullName>
        <active>true</active>

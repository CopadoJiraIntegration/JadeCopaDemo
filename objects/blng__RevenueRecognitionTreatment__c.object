    <validationRules>
        <fullName>blng__AGREEMENT_ASSOC_VALIDONLYFOROPACTIVATION</fullName>
        <active>true</active>
        <description>Revenue Agreement Association setting is only valid if Revenue Schedules are created on Order Product Activation.</description>
        <errorConditionFormula>OR(
            AND(
            (!ISPICKVAL(blng__RevenueScheduleCreationAction__c, &apos;Order Activation&apos;)),
            (ISPICKVAL(blng__RevenueAgreementAssociation__c, &apos;New Agreement&apos;))
            ),
            AND(
            (!ISPICKVAL(blng__RevenueScheduleCreationAction__c, &apos;Order Activation&apos;)),
            (ISPICKVAL(blng__RevenueAgreementAssociation__c, &apos;Revised Order Product&apos;))
            )
            )</errorConditionFormula>
        <errorMessage>If Revenue Schedule Creation is not Order Product Activation, Revenue Agreement Association must be “Not Applicable” or “Manual”</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>blng__Bookings_Amount_Only_If_Order_Activation</fullName>
        <active>true</active>
        <description>Revenue Schedule Amount can be Bookings Amount only if Revenue Schedule Creation Action = Order Activation.</description>
        <errorConditionFormula>AND(
        (!ISPICKVAL(blng__RevenueScheduleCreationAction__c, &apos;Order Activation&apos;)),
        (ISPICKVAL(blng__RevenueScheduleAmount__c, &apos;Bookings Amount&apos;))
        )</errorConditionFormula>
        <errorMessage>To set the revenue schedule amount to Booking Amount, first set the revenue schedule creation action to Order Product Activation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>blng__Flat_Amount_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(blng__FlatAmount__c) &amp;&amp; ISPICKVAL( blng__Type__c, &apos;Flat Amount&apos;)</errorConditionFormula>
        <errorDisplayField>blng__FlatAmount__c</errorDisplayField>
        <errorMessage>If type is &quot;Flat Amount&quot;, Flat Amount is mandatory to populate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>blng__Flat_Amount_Value_more_than_zero</fullName>
        <active>true</active>
        <description>Amount must be equal to or greater than zero</description>
        <errorConditionFormula>IF((!ISBLANK(blng__FlatAmount__c)) &amp;&amp; blng__FlatAmount__c &lt; 0, True, False)</errorConditionFormula>
        <errorMessage>Flat Amount Cannot be less than Zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>blng__PERCENTAGE_MUST_BE_LESS_THAN_HUNDRED</fullName>
        <active>true</active>
        <description>PERCENTAGE MUST BE LESS THAN HUNDRED</description>
        <errorConditionFormula>blng__Percentage__c  &gt; 1.0</errorConditionFormula>
        <errorMessage>Percentage Cannot be greater than 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>blng__Percentage_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(blng__Percentage__c) &amp;&amp; ISPICKVAL( blng__Type__c, &apos;Percentage&apos;)</errorConditionFormula>
        <errorDisplayField>blng__Percentage__c</errorDisplayField>
        <errorMessage>If type is &quot;Percentage&quot;, Percentage is mandatory to populate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>blng__Percentage_Value_more_than_zero</fullName>
        <active>true</active>
        <description>Percentage must be equal to or greater than zero</description>
        <errorConditionFormula>IF((!ISBLANK(blng__Percentage__c))  &amp;&amp;  blng__Percentage__c &lt;= 0, True, False)</errorConditionFormula>
        <errorMessage>Percentage must be greater than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>blng__Processing_Order_Cannot_be_negative</fullName>
        <active>true</active>
        <description>Processing Order Cannot be Negative</description>
        <errorConditionFormula>blng__ProcessingOrder__c  &lt; 0</errorConditionFormula>
        <errorMessage>Processing Order cannot be Negative</errorMessage>
    </validationRules>

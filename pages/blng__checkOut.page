              $(".cardTypeClass").after(html);
            }else if(value == 'Ebay'){
              var html = "<td><img src='{!URLFOR($Resource.blng_slds, 'assets/images/Ebay.png')}'/></td>";
              $(".cardTypeClass").after(html);
